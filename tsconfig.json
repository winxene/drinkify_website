{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/**/*.ts", "config/firebaseConfig.ts"],
  "exclude": ["node_modules"],
  "files": ["src/pages/_app.tsx",  "src/pages/index.tsx", "src/pages/api/hello.ts", "config/firebaseConfig.ts"]
}
